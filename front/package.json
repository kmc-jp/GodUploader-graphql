{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/sortable": "^4.0.0",
    "@dnd-kit/utilities": "^2.0.0",
    "@popperjs/core": "^2.9.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/bootstrap": "^5.0.16",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.2",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-relay": "^13.0.0",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/relay-runtime": "^13.0.0",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "clsx": "^1.1.1",
    "konva": "^8.0.4",
    "react": "17",
    "react-dom": "17",
    "react-helmet": "^6.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-konva": "^17.0.2-4",
    "react-relay": "^14.1.0",
    "react-router": "5.3.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "5.3.3",
    "react-scripts": "5.0.0",
    "react-string-replace": "^0.4.4",
    "react-use": "^17.2.4",
    "relay-runtime": "^14.1.0",
    "typescript": "^4.4.3",
    "use-measure": "^0.3.0"
  },
  "scripts": {
    "start": "env REACT_APP_REVISION=`git rev-parse HEAD` REACT_APP_BUILT_AT=`date +%s` vite",
    "build": "tsc && env REACT_APP_BUILT_AT=`date +%s` vite build",
    "test": "react-scripts test",
    "codegen": "graphql-codegen",
    "relay-compiler": "relay-compiler"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    }
  },
  "prettier": {
    "importOrder": [
      "^[./]"
    ],
    "importOrderSeparation": true
  },
  "relay": {
    "schema": "./schema.graphql",
    "src": "./src",
    "language": "typescript",
    "eagerEsModules": true,
    "customScalars": {
      "DateTime": "string",
      "Upload": "null"
    }
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@testing-library/react-hooks": "^7.0.0",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-relay": "^14.1.0",
    "cra-bundle-analyzer": "^0.1.0",
    "graphql": "^15.5.0",
    "prettier": "^2.3.1",
    "relay-compiler": "^14.1.0",
    "vite": "^4.1.1",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vite-plugin-relay": "^2.0.0"
  },
  "resolutions": {
    "@types/relay-runtime": "~13.0.0"
  }
}
