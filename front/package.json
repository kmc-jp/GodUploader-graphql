{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^3.1.1",
    "@dnd-kit/sortable": "^4.0.0",
    "@dnd-kit/utilities": "^2.0.0",
    "@popperjs/core": "^2.9.2",
    "@types/bootstrap": "^5.0.16",
    "@types/node": "^20",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-relay": "^13.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-config": "^5.0.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/relay-runtime": "^13.0.0",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "clsx": "^1.1.1",
    "konva": "^8.0.4",
    "react": "18",
    "react-dom": "18",
    "react-helmet": "^6.1.0",
    "react-infinite-scroller": "^1.2.6",
    "react-konva": "^18.2.10",
    "react-relay": "^14.1.0",
    "react-router": "5",
    "react-router-config": "5",
    "react-router-dom": "5",
    "react-string-replace": "^0.4.4",
    "react-use": "^17.4.0",
    "relay-runtime": "^14.1.0"
  },
  "scripts": {
    "start": "env VITE_REVISION=`git rev-parse HEAD` VITE_BUILT_AT=`date +%s` vite",
    "build": "tsc && env VITE_REVISION=`git rev-parse HEAD` VITE_BUILT_AT=`date +%s` vite build",
    "test": "vitest",
    "codegen": "graphql-codegen",
    "relay-compiler": "relay-compiler",
    "eslint": "eslint",
    "tsc": "tsc"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    }
  },
  "prettier": {
    "importOrder": [
      "^[./]"
    ],
    "importOrderSeparation": true
  },
  "relay": {
    "schema": "./schema.graphql",
    "src": "./src",
    "language": "typescript",
    "eagerEsModules": true,
    "customScalars": {
      "DateTime": "string",
      "Upload": "null"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "^8.33.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^15.5.0",
    "happy-dom": "^20.0.2",
    "prettier": "^2.3.1",
    "react-test-renderer": "^18.2.0",
    "relay-compiler": "^14.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-relay": "^2.0.0",
    "vitest": "^4.0.16"
  },
  "resolutions": {
    "@types/relay-runtime": "~13.0.0"
  }
}
