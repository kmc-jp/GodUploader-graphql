{
    "python.pythonPath": "api/.venv/bin/python",
    "python.formatting.provider": "black",

    "[python]": {
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
        "source.organizeImports": true,
      },
    },
    "[typescript]": {
      "editor.formatOnSave": true,
    },
    "[typescriptreact]": {
      "editor.formatOnSave": true,
    },
}
